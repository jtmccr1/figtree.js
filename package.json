{
  "name": "figtreejs",
  "type": "module",
  "version": "0.1.0",
  "main": "dist/figtree.cjs.js",
  "module": "dist/figtree.esm.js",
  "browser": "dist/figtree.umd.js",
  "keywords": [
    "phylogeny",
    "tree"
  ],
  "ignore": [
    "index.html"
  ],
  "dependencies": {
    "abs-svg-path": "^0.1.1",
    "d3-array": "^3.2.4",
    "d3-ease": "^3.0.1",
    "d3-interpolate-path": "^2.3.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-selection-multi": "^0.3.0",
    "d3-shape": "^3.2.0",
    "d3-transition": "^3.0.1",
    "font-awesome": "4.x",
    "immer": "^10.0.4",
    "normalize-svg-path": "^1.1.0",
    "parse-svg-path": "^0.1.2",
    "roughjs": "^3.1.0",
    "sphinx": "^0.1.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0",
    "rollup": "^3.29.2",
    "rollup-plugin-dts": "^6.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "docs": "jsdoc -r src/* -d ./doc/",
    "build": "rollup -c --bundleConfigAsCjs",
    "build:watch": "rollup -c --bundleConfigAsCjs -w",
    "dev": "rollup -c --bundleConfigAsCjs -w",
    "test": "node --experimental-vm-modules  node_modules/jest/bin/jest.js --detectOpenHandles",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --detectOpenHandles"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "transform": {}  }
